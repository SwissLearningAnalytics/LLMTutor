{
  "name": "swiss-learning-analytics-llm-tutor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "MPL-2.0",
  "scripts": {
    "dev": "VITE_APP_VERSION=$(git describe --tags) vite dev --mode dev-local",
    "build": "VITE_APP_VERSION=$(git describe --tags) vite build --mode dev-local",
    "lint": "biome check",
    "build:dev": "VITE_APP_VERSION=$(git describe --tags) vite build --mode dev",
    "build:prod": "VITE_APP_VERSION=$(git describe --tags) vite build --mode prod",
    "format": "prettier ./src --write",
    "lint:fix": "biome check --fix",
    "lint:fix:unsafe": "biome check --fix --unsafe",
    "db:up": "docker compose -f docker-compose/stack.yml up -d",
    "db:down": "docker compose -f docker-compose/stack.yml down",
    "db:studio": "npx drizzle-kit studio",
    "db:push": "npx drizzle-kit push",
    "db:migrations:generate": "npx drizzle-kit generate",
    "db:migrations:apply": "npx drizzle-kit migrate",
    "generate:tutors-index": "tsx generate-tutors-index.ts",
    "prepare": "pnpm generate:tutors-index"
  },
  "dependencies": {
    "@ai-sdk/openai": "1.3.22",
    "@ai-sdk/react": "1.2.12",
    "@fontsource-variable/hanken-grotesk": "^5.2.6",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-popover": "1.1.14",
    "@radix-ui/react-slot": "1.2.3",
    "@tailwindcss/postcss": "4.1.8",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/react-query": "5.80.6",
    "@tanstack/react-router": "1.131.27",
    "@tanstack/react-start": "1.131.27",
    "ai": "4.3.16",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "dotenv": "16.5.0",
    "drizzle-orm": "0.43.1",
    "framer-motion": "12.16.0",
    "install": "^0.13.0",
    "katex": "^0.16.22",
    "lucide-react": "0.513.0",
    "marked": "^16.1.1",
    "nanoid": "5.1.5",
    "ollama-ai-provider": "1.2.0",
    "pg": "8.16.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "tailwind-merge": "3.3.0",
    "tailwindcss-animate": "1.0.7",
    "vite": "^7.1.3",
    "yaml": "2.8.1",
    "zod": "3.25.53",
    "zustand": "5.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@modyfi/vite-plugin-yaml": "1.1.1",
    "@tailwindcss/container-queries": "0.1.1",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query-devtools": "5.80.6",
    "@tanstack/react-router-devtools": "1.131.27",
    "@tanstack/router-plugin": "1.131.27",
    "@types/node": "22.15.30",
    "@types/pg": "8.15.2",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.1",
    "autoprefixer": "10.4.21",
    "drizzle-kit": "0.31.1",
    "postcss": "8.5.4",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.12",
    "tailwindcss": "3.4.17",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "vite-plugin-svgr": "4.3.0",
    "vite-tsconfig-paths": "5.1.4"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "tailwindConfig": "./tailwind.config.cjs"
  }
}
